        <class_name>Processor</class_name>

        <class_source><![CDATA[
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// create output row with room for one more field
    r = createOutputRow(r, r.length + 1);
	
	// get the coords
    Double lat = get(Fields.In, "LATITUDE").getNumber(r);
	Double lon = get(Fields.In, "LONGITUDE").getNumber(r);

	// get the name
	String name = get(Fields.In, "PLACE_NAME").getString(r);

	// get the cc
	String cc = get(Fields.In, "ISO2_COUNTRY_CODE").getString(r);

	// get the feature type
	String type = get(Fields.In, "FEATURE_CODE").getString(r);


	// create the key by concating name,country_code,type, latitude and longitude
	String keyString = name + "-" + cc +"-" + type +"-" + lat +"-" + lon;
	Long key = ((Integer)keyString.hashCode()).longValue();

 	// Set conflate_key field
    get(Fields.Out, "CONFLATE_KEY").setValue(r, key);



	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
