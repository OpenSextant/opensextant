        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }
	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, r.length + 1);
	
	// get the source field
	String srcOrig = get(Fields.In, "SOURCE").getString(r);

	if(srcOrig == null){ 
		srcOrig = "ADHOC";
	}

	String src = srcOrig;



	// get the place name
    String name = get(Fields.In, "PLACE_NAME").getString(r);
	String nameOrig = name;

	// all types of whitespace -> single space
	name = name.replaceAll("\\p{Z}+"," ").trim();

/*
	// fix phantoms, regular and busted
	if( name.contains("((") && name.contains("))")  ){
		name = name.replaceAll("\\(\\("," ").replaceAll("\\)\\)"," ").replaceAll(" +"," ").trim();
	}

*/
	// remove trailing numbers in parens
	if( name.trim().matches(".*\\([0-9]+\\)$") ){
		name = name.replaceAll("\\([0-9]+\\)$","").trim();
	}


	// convert underbars to space
	if( name.contains("_")){
		name = name.replaceAll("_"," ").replaceAll(" +"," ").trim();
	}



 	// Set new value for name field and source if any changes made

	if( !name.equals(nameOrig) ){
		src = srcOrig + "-AUTOFIXED";
   	 	get(Fields.Out, "PLACE_NAME").setValue(r, name);
   	 	get(Fields.Out, "SOURCE").setValue(r, src);
	}

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
