    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }
	
	// create output row
    r = createOutputRow(r, r.length);
	
	// get the error and residue fields
	String error = get(Fields.In, "Error").getString(r);
	if(error == null){ error = "";}

	String residue = get(Fields.In, "ErrorDetails").getString(r);
	if( residue == null) {residue = "";}

	// get the place name
    String name = get(Fields.In, "PLACE_NAME").getString(r);

	// look for error conditions
	// if found append error name to error field
	// if appropriate, append some info to residue field

	// look for "phantom" names i.e. "((name))
	if( name.matches("\\(\\(.*\\)\\)") ){
		error =  error + "," + "PhantomLocation";
	}

	// look for busted "phantom" names i.e. "namePart))((namePart"
	if( !error.contains("Phantom") && name.contains("((") && name.contains("))")  ){
		error =  error + "," + "PhantomLocation-Busted";
	}


	// name is all digits
	if( name.trim().matches("\\p{N}+")  ){
		error =  error + "," + "AllNumbers";
	}


	// name followed by parened number e.g. name (2)
	if( name.trim().matches(".*\\([0-9]+\\)$") ){
		error =  error + "," + "NumbersAtEnd";
	}

	// name has number but not all numbers or parened numbers
	if( !error.contains("NumbersAtEnd") && !error.contains("AllNumbers") &&  name.matches(".*\\p{N}+.*") ){
		error =  error + "," + "NumbersInName";
	}

	// names has commas,colon or semicolon
	if (name.matches(".*[:;,].*")) {
		error =  error + "," + "ExtraPunctComma";
	}

	// brackets or rivets
	if (name.matches(".*[\\[\\]{}<>].*")) {
		error =  error + "," + "ExtraPunctBracket";
	}

	// name has parens but not a phantom
	if (!error.contains("Phantom") && name.matches(".*[\\(\\)].*")) {
		error =  error + "," + "ExtraPunctBracket";
	}

	// name has some sort of slash
	if (name.matches(".*[/\\\\].*")) {
		error =  error + "," + "ExtraPunctSlash";
	}

	// name has misc punct
	if (name.matches(".*[\\*\\+_\\^\\&=¿!?].*")) {
		error =  error + "," + "ExtraPunctOther";
	}

	// name has quotes
	if (name.matches(".*[\"].*")) {
		error =  error + "," + "ExtraPunctQuotes";
	}

	// namde has a pound sign not folowed by a number
	if (name.matches(".*#\\D+.*")) {
		error =  error + "," + "ExtraPunctPound";
	}

	// name has extra whitspace of some kind
	if (name.compareTo(name.replaceAll("\\p{Z}+", " ").trim()) != 0) {
		error =  error + "," + "ExtraWhiteSpace";
	}

	// name is an explicit NULL
	if (name.toUpperCase().matches(".*(^|\\s)NULL(\\s|$).*")  ||  name.toUpperCase().matches("<NULL>")     )   {
		error =  error + "," + "NullInName";
	}


	// name looks like a roman number
	if (name.trim().matches("[IXVLCDM]+")  )  {
		error =  error + "," + "RomanNumber";
	}

	// name is some form of UNKNOWN
	if (name.toUpperCase().contains("UNKNOWN") || name.toUpperCase().contains("NOT KNOWN")  )  {
		error =  error + "," + "UnknownName";
	}


	// name looks likes name1 see name2
	if (name.toLowerCase().trim().matches(".+\\ssee\\s.+")  )  {
		error =  error + "," + "SeeReference";
	}

	// name is marked as historical i.e "name (historical)
	if (name.toUpperCase().matches(".*[\\(\\[\\{]HISTORICAL[\\)\\]\\}]$")  ) {
		error =  error + "," + "HistoricName";
	}

	// name is marked as inactive i.e "name (inactive)
	if (name.toUpperCase().contains("INACTIVE")  ) {
		error =  error + "," + "HistoricName";
	}


	// name is too short (just simple Latin chars)
	if(name.length() < 3 && name.matches("[-a-zA-Z0-9'`]+")  ){
		error =  error + "," + "TooShortName";
	}

	// diagnostics, remove for production
	// catchall: not caught above but contains something other then letters, spaces and "normal" punct
	if( error.length() <1 && !name.matches("[-­\\p{L}\\p{M}`'‘’´\.\\s]+") ){

		residue = name.replaceAll("[-­\\p{L}\\p{M}`'‘’´\.\\s]+","");
		error =  error + "," + "CatchallUnicode";
	}



 	// Set error and residue fields
    get(Fields.Out, "Error").setValue(r, error);
 	get(Fields.Out, "ErrorDetails").setValue(r, residue);


	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
