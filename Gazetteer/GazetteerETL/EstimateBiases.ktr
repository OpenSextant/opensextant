        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

    r = createOutputRow(r, data.outputRowMeta.size());

	String caseSeq = get(Fields.In, "caseSequence").getString(r);
	String typeSeq = get(Fields.In, "tokenTypeSequence").getString(r);
	String scriptSeq = get(Fields.In, "scriptSequence").getString(r);
	String suffixSeq = get(Fields.In, "suffixSequence").getString(r);

	String[] cases = caseSeq.split(",");
	String[] types = typeSeq.split(",");
	String[] scripts = scriptSeq.split(",");
	String[] suffixes = suffixSeq.split(",");

	// all sequences/array should be same. size need to check
	int seqLen = cases.length;

	String namePattern = "";
	String[] namePatternSeq = new String[seqLen];

	for(int i=0; i < seqLen; i++){
	
		String c = cases[i].trim();
		String t = types[i].trim();
		String scr = scripts[i].trim();
		String suff = suffixes[i].trim();

		// initial type is based on case
		namePatternSeq[i] = c;

		// if any suffix type, use that
		if(!suff.trim().equalsIgnoreCase("Word")){
	  		namePatternSeq[i] = suff;
		}

		// if any vocab type, use that
		if(!t.trim().equalsIgnoreCase("Word")){
	  		namePatternSeq[i] = t;
		}
	
		// if any script type, use that
		if(!scr.trim().equalsIgnoreCase("[LATIN]")){
	  		namePatternSeq[i] = "NonLatin";
		}
	
	}

	for(int x=0; x < seqLen; x++){
		namePattern = namePattern +  "," + namePatternSeq[x];
	}
	
	namePattern = namePattern.replaceAll("^,","");


	get(Fields.Out, "namePattern").setValue(r, namePattern);

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}


