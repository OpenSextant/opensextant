        <class_source><![CDATA[
static Double boostConst = 0.0;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
		boostConst = Double.parseDouble(getParameter("featureTypeBoost"));
    }

    r = createOutputRow(r, data.outputRowMeta.size());


	Boolean isExceptionPattern = false;
	Double boost =0.0;

	Double minedScore = get(Fields.In, "minedScore").getNumber(r);
	Double patternWeight = get(Fields.In, "patternWeight").getNumber(r);

	String namePattern = get(Fields.In, "namePattern").getString(r);

	String placeName = get(Fields.In, "PLACE_NAME").getString(r);
	String fClass = get(Fields.In, "FEATURE_CLASS").getString(r);
	String fCode = get(Fields.In, "FEATURE_CODE").getString(r);
	String cc = get(Fields.In, "FIPS_COUNTRY_CODE").getString(r);
	String nameType = get(Fields.In, "NAME_TYPE").getString(r);
	Long tokenCount = get(Fields.In, "tokenCount").getInteger(r);

	Long wordStatsTotal = get(Fields.In, "wordStatsTotal").getInteger(r);



	if(namePattern.matches("Common|Person|GeoFeature")){
		isExceptionPattern= true;
	}

	Boolean isAbbrev = false;
	if(nameType.matches("abbrev|code")){
		isAbbrev = true;
	}



	// set a boost value based on feature type
	// only boost records: not abbreviations, not exception pattern and with neutral pattern and mined scores
	if(!isAbbrev && !isExceptionPattern && patternWeight == 0.0 && (minedScore == null || minedScore == 0.0 ) ){
		// boost continent,countries, national capitals and US states
		if(fCode.matches("CONT") |fCode.matches("PCLI") | fCode.matches("PPLC") | ( cc.equals("US") &&  fCode.matches("ADM1"))){
		  boost = boostConst;
		}
	}

	get(Fields.Out, "featureTypeWeight").setValue(r, boost);


	// long name patterns with neutral score get boost
	if(patternWeight == 0.0 && tokenCount > 3){
		patternWeight = 1.0;
		get(Fields.Out, "patternWeight").setValue(r, patternWeight);
	}

	// adjust abbreviations based on namepatternv versus gazetteer nametype
	if(isAbbrev && minedScore == null){
		if(namePattern.matches("Abbrev")){ // is abbreviation and looks like abbreviation-> boost
			get(Fields.Out, "minedScore").setValue(r, 0.01);
		 }else{// is abbreviation but doesnt looks like abbreviation-> neutral
			get(Fields.Out, "minedScore").setValue(r, 0.0);
		}
	}





	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
