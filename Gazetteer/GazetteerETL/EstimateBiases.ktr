        <class_source><![CDATA[import java.util.regex.Pattern;
import java.util.*;

static Pattern matchPattern;
static String tmpPattern = "";

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{

	// get all of the regexes and their categories
	
    if (first){
        first = false;
		RowSet infoStream = findInfoRowSet("affixes");
		
		Object[] infoRow = null;
		
		while((infoRow = getRowFrom(infoStream)) != null){
			String regex = get(Fields.Info, "Affix").getString(infoRow);
			tmpPattern = regex + "|" + tmpPattern; 
		}

		tmpPattern = tmpPattern.replaceAll("\\|$","");


	    matchPattern = Pattern.compile(tmpPattern);

    }// end init



	Object[] r = getRow();

	if (r == null) {
		setOutputDone();
		return false;
	}
	   r = createOutputRow(r, data.outputRowMeta.size());


	String cat = "Word";
	String token = get(Fields.In, "token").getString(r);

	if(matchPattern.matcher(token).matches()){
		cat = "PLACE";
	}

	get(Fields.Out, "SuffixCategory").setValue(r, cat);
	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}

]]></class_source>
