  <step>
    <name>Process Case Info</name>
    <type>UserDefinedJavaClass</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>

    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

   r = createOutputRow(r, data.outputRowMeta.size());

	String gram = get(Fields.In, "Ngram").getString(r);
	Double conf = get(Fields.In, "CaseConfidence").getNumber(r);
	Long total  = get(Fields.In, "Total").getInteger(r);
	String decision = get(Fields.In, "Decision").getString(r);

	Double confThresh =	Double.parseDouble(getParameter("confidenceThreshold"));
	Long countThresh = Long.parseLong(getParameter("countThreshold"));


	// token never seen in corpus, could guess Proper. 
	if(decision == null){
		decision = "Ambig";
	} 


	// set low frequency/confidence records to ambigous
	if(conf != null && conf < confThresh){
		decision = "Ambig";
	}

	 if(total != null && total < countThresh ){
		decision = "Ambig";
	}


	// change/merge case categories
	if( decision.equals("initial")|| decision.equals("mixed")){
		decision = "Proper";
	}  

	if(decision.equals("lower")){
		decision = "Common";
	}  

	if( decision.equals("upper") || gram.matches("[bcdfghjklmnpqrstvwxz]+") ){
		decision = "Abbrev";
	}  


	get(Fields.Out, "Decision").setValue(r, decision);

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
