        <class_source><![CDATA[
static Long commonThresh = 0L;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
		commonThresh = Long.parseLong(getParameter("veryCommonThresh"));
    }

    r = createOutputRow(r, data.outputRowMeta.size());

	String token = get(Fields.In, "token").getString(r);
	String decision = get(Fields.In, "caseDecision").getString(r);
	Long wordStatsTotal = get(Fields.In, "wordStatsTotal").getInteger(r);
	String nameType = get(Fields.In, "NAME_TYPE").getString(r);

	boolean isAbbrev = false;
	if(nameType.matches("abbrev|code")){
		isAbbrev = true;
	}

	// if unseen in corpus
	if(wordStatsTotal == 0L){
		// if all consonants
		if( token.matches("[bcdfghjklmnpqrstvwxz]+") ){    
			get(Fields.In, "caseDecision").setValue(r, "Abbrev");
		} 

/*
		// if all digits
		if( token.matches("[0-9]+") ){
			get(Fields.In, "caseDecision").setValue(r, "Number");
		} 
*/
	}


	// very frequent,(mostly)all lower words, but not abbrev
	if(!isAbbrev && wordStatsTotal > commonThresh && decision.matches("Common|Ambig")){ 
		get(Fields.In, "caseDecision").setValue(r, "Little");
	}


	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
