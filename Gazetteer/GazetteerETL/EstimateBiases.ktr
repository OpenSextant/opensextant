        <class_source><![CDATA[

// the weighting factors for the id bais calculations
static double WellKnownWeight = 0.51;
static double FeatureTypeWeight = 0.45;
static double CountryWeight = 0.04;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();

	if (r == null) {
		setOutputDone();
		return false;
	}
    r = createOutputRow(r, data.outputRowMeta.size());

	// get the bias factors
	Double wellknown_id_bias = get(Fields.In, "wellknown_id_bias").getNumber(r);
	if(wellknown_id_bias == null) {wellknown_id_bias =0.0;}

	Double featuretype_id_bias = get(Fields.In, "featuretype_id_bias").getNumber(r);
	if(featuretype_id_bias == null) { featuretype_id_bias = 0.0;}

	Double country_id_bias = get(Fields.In, "COUNTRY_ID_BIAS").getNumber(r);
    if(country_id_bias == null) { country_id_bias = 0.0;}


	// calculate the total bias
	 Double total_id_bias = WellKnownWeight * wellknown_id_bias + FeatureTypeWeight*featuretype_id_bias + CountryWeight*country_id_bias;

	// set the id bias value
	get(Fields.Out, "PLACE_ID_BIAS").setValue(r, total_id_bias);

			
	putRow(data.outputRowMeta, r);

	return true;
}



]]></class_source>
