        <class_source><![CDATA[

static double deltaThresh = 0.5;



public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();

	if (r == null) {
		setOutputDone();
		return false;
	}
    r = createOutputRow(r, data.outputRowMeta.size());

	// get the basename
	String baseName = get(Fields.In, "baseName").getString(r);

	// get the geolocation for this record
	Double recLat = get(Fields.In, "LATITUDE").getNumber(r);
	Double recLong = get(Fields.In, "LONGITUDE").getNumber(r);

	// get the geolocation of the well knwon place which has this name
	Double wellKnownLat = get(Fields.In, "BigCityLat").getNumber(r);
	Double wellKnownLong = get(Fields.In, "BigCityLong").getNumber(r);

	// if this record is within threshold of the well known place of same name
	if(wellKnownLat != null && wellKnownLong != null){
		// if well known(lat,long) close to record(lat,long)
		Double delta = Math.sqrt( Math.pow((recLat - wellKnownLat),2) + Math.pow((recLong - wellKnownLong),2) ); 

		// if distanace greater than threshold, not the well known place, set to 0.0
		if( delta >= deltaThresh) {
			get(Fields.Out, "wellknown_id_bias").setValue(r, 0.0);
		}

	}
		
	putRow(data.outputRowMeta, r);


	return true;
}



]]></class_source>
