        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }
    r = createOutputRow(r, data.outputRowMeta.size());

	Double conf = 0.0;

	Double minedScore = get(Fields.In, "minedScore").getNumber(r);
	Double patternWeight = get(Fields.In, "patternWeight").getNumber(r);
	Double featureTypeWeight = get(Fields.In, "featureTypeWeight").getNumber(r);
	Double wellknown_id_bias = get(Fields.In, "wellknown_id_bias").getNumber(r);
	
	// if mined score is valid, use that
	if(minedScore != null){
		conf = minedScore;
		get(Fields.Out, "PLACE_NAME_BIAS").setValue(r, conf);

		// Send the row on to the next step.
    	putRow(data.outputRowMeta, r);
		return true;
	}

	//no mined score, so use a combination of patternWeight and featureTypeWeight

	//compare patternWeight and featureTypeWeight to see if they have the same polarity
	if(patternWeight * featureTypeWeight < 0.0){// different polarities, set to neutral
		conf = 0.0;
	}else{ //same polarity, set to average 
			conf = (patternWeight + featureTypeWeight)/2.0; 
	}	
	

	get(Fields.Out, "PLACE_NAME_BIAS").setValue(r, conf);

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
