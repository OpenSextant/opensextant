        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

    r = createOutputRow(r, data.outputRowMeta.size());

	String type = "name";

	String name = get(Fields.In, "PLACE_NAME").getString(r);

	// look for names which look like abbreviations
	// ABC or A.B.C. or Abc. or A B C or PDQ (no vowels)
	if(name.matches("[A-Z]{2,4}|([A-Z]\\.){2,3}|[A-Z][a-z]{1,3}\\.|([A-Z] )+[A-Z]|[B-D,F-H,J-N,P-T,V-X,Z]+")){
			type = "abbrev";
	 }	

	get(Fields.Out, "NAME_TYPE").setValue(r, type);


	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
]]></class_source>
