    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
 	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    r = createOutputRow(r, data.outputRowMeta.size());

	String cc = get(Fields.In, "COUNTRY_ALPHA").getString(r);
	if (cc == null) {

		// If CC is missing, assume it is US. This is probably a good assumption for USGS data.
		// Fire departments, schools, farms, ranches, cemeteries, and quarrys constitute the
		// majority of places without country codes.
		cc = "US";
	}

	Long a1 = get(Fields.In, "STATE_NUMERIC").getInteger(r);
	if (a1 != null) {
		int admin1 = a1.intValue();
		Long a2 = get(Fields.In, "COUNTY_NUMERIC").getInteger(r);

	

		// Use admin1 to select the right CC

		if (cc.equals("US")) {

			switch (admin1) {	

				case 60:	cc = "AQ";		// American Somoa
							break;

				case 64:	cc = "FM";		// Federated States of Micronesia
							break;

				case 66:	cc = "GQ";		// Guam
							break;
				
				case 68:	cc = "RM";		// Marshall Islands
							break;

				case 69:	cc = "CQ";		// Northern Mariana Islands
							break;

				case 70:	cc = "PS";		// Palau
							break;

				case 72:	cc = "RQ";		// Puerto Rico
							break;

				case 78:	cc = "VQ";		// US Virgin Islands
							break;

				case 74:	if (a2 != null) {				// US Minor Outlying Islands
								int admin2 = a2.intValue();			
								switch (admin2) {			// Use admin2 to select the CC

									case 100:	cc = "HQ";	// Howland Island
												break;

									case 50:	cc = "HQ";	// Include Baker Island with Howland Island
												break;

									case 150:	cc = "DQ";	// Jarvis Island
												break;

									case 200:	cc = "JQ";	// Johnston Atoll
												break;

									case 250:	cc = "KQ";	// Kingman Reef
												break;

									case 300:	cc = "MQ";	// Midway Islands
												break;

									case 350:	cc = "BQ";	// Navassa Island
												break;

									case 400:	cc = "LQ";	// Palmyra Atoll
												break;

									case 450:	cc = "WQ";	// Wake Island
												break;

									default:	break;
								}
						}
						break;

				default: break;
			}

		}
	}

	// set the country code
	get(Fields.Out, "COUNTRY_ALPHA").setValue(r, cc);

	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

    return true;
}]]></class_source>
        </definition>
