        <class_name>Processor</class_name>

        <class_source><![CDATA[
import java.util.*;


// this step
// 1) creates the ADMIN1ID by concatenating the "main" country code and the admin1 value (ADMIN1ID)
// 2) creates a list of distinct country codes from the cc1 and cc2 fields (COUNTRYCODELIST)
// 3) creates a list from all the distinct names from this record (NAMELIST)

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}


	// create an ouput row with room for four new field
    r = createOutputRow(r, r.length + 4);


	Long featID = get(Fields.In, "FEATURE_ID").getInteger(r);

	// create a PLACE_ID by concatenating the UFI with a constant

	String placeID = "USGS" + featID.toString();
	
	// set the placeID
	get(Fields.Out, "PLACE_ID").setValue(r, placeID);

	String name = get(Fields.In, "FEATURE_NAME").getString(r);
	String variant = get(Fields.In, "FEATURE_NAME_VARIANT").getString(r);

	if(variant != null){
		name = name + "|" + variant;
	}



	// create country codes, admin_level_ and admin_level_2 codes

	String cc = get(Fields.In, "COUNTRY_ALPHA").getString(r);
	if(cc == null) {
		cc = "US"; // bad assumption, how to check? Fixed by "CountryAdjusts" earlier in flow 
	}


	Long admin1Value = get(Fields.In, "STATE_NUMERIC").getInteger(r);
	Long admin2Value = get(Fields.In, "COUNTY_NUMERIC").getInteger(r);

	if(admin1Value == null) {
		admin1Value = 0L;
	}

	if(admin2Value == null) {
		admin2Value = 0L;
	}

	String admin1 = admin1Value.toString();
	String admin2 = admin2Value.toString();


	if (admin1.length() == 1){
		admin1 = "0" + admin1;
	}

	if (admin2.length() == 1){
		admin2 = "000" + admin2;
	}

	if (admin2.length() == 2){
		admin2 = "00" + admin2;
	}

	if (admin2.length() == 3){
		admin2 = "0" + admin2;
	}



	String admin1Code = cc+admin1;
	String admin2Code = cc+admin1+"."+ admin2;



	// merge the feature class and unit type fields


	String featClass = get(Fields.In, "FEATURE_CLASS").getString(r);
	String unitType = get(Fields.In, "UNIT_TYPE").getString(r);

	if(featClass != null && featClass.equals("Civil") && unitType != null   ){
		featClass = featClass + "-" + unitType;
		
	} 


	// set the NAMELIST output field
    get(Fields.Out, "NAMELIST").setValue(r, name);

	// set the country code
	get(Fields.Out, "COUNTRY_CODE").setValue(r, cc);

	// set the admin level codes
    get(Fields.Out, "ADMIN_LEVEL_1").setValue(r, admin1Code);
    get(Fields.Out, "ADMIN_LEVEL_2").setValue(r, admin2Code);

	// set the feature class field 
	get(Fields.Out, "GNIS_FEATURE_CLASS").setValue(r, featClass);

	// set the source ids
	get(Fields.Out, "SOURCE_FEATURE_ID").setValue(r, featID);
	get(Fields.Out, "SOURCE_NAME_ID").setValue(r, "");




	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);


	return true;
}
